@model Vidly.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customer Details</h2>

 <h3>@Model.Name</h3>
<ul>
    <li>Membership Type: <strong>@Model.MembershipType.Name</strong></li>
    @if (@Model.BirthDate.HasValue)
    {
        <li>Birth Date: <strong>@Model.BirthDate.Value.ToShortDateString()</strong></li>
    }
    
</ul>
            
     
    
 

